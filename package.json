{
  "name": "freshbooks-kforce-worker",
  "version": "1.0.0",
  "description": "FreshBooks to Kforce invoice converter",
  "main": "dist/index.js",
  "scripts": {
    "dev:worker": "wrangler dev --local",
    "dev:tunnel": "cloudflared tunnel run quantum-nexus-7249",
    "dev": "concurrently -n worker,tunnel \"npm run dev:worker\" \"npm run dev:tunnel\"",
    "dev:restart-worker": "concurrently -n worker,tunnel \"npm run dev:worker\" \"npm run dev:tunnel\" --kill-others-on-fail",
    "build": "wrangler build",
    "deploy": "wrangler deploy",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "itty-router": "^4.0.27",
    "itty-router-extras": "^0.4.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "@peculiar/webcrypto": "^1.4.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.13.1",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "wrangler": "^3.28.1"
  }
}
